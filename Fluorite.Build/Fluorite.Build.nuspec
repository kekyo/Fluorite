<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Fluorite.Build</id>
    <version>$PackageVersion$</version>
    <authors>Kouji Matsui (@kozy_kekyo, @kekyo2)</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">Apache-2.0</license>
    <licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
    <icon>Fluorite.100.png</icon>
    <projectUrl>https://github.com/kekyo/Fluorite.git</projectUrl>
    <description>Simplest and fully-customizable RPC standalone infrastructure.</description>
    <releaseNotes>$PackageReleaseNotes$</releaseNotes>
    <copyright>Copyright (c) 2021 Kouji Matsui</copyright>
    <tags>simple async standalone bidirectional websocket json transport rpc-server rpc-client full-duplex complementary</tags>
    <repository type="git" url="https://github.com/kekyo/Fluorite.git" branch="$RepositoryBranch$" commit="$RepositoryCommit$" />
  </metadata>
  <files>
    <file src="..\Images\Fluorite.100.png" target="/" />
    <file src="build\*.*" target="build" />
    <file src="buildMultiTargeting\*.*" target="buildMultiTargeting" />
    <file src="bin\$Configuration$\net461\*.*" target="tools/net461" />
    <file src="bin\$Configuration$\netcoreapp2.0\*.*" target="tools/netcoreapp2.0" />
    <file src="bin\$Configuration$\netcoreapp2.1\*.*" target="tools/netcoreapp2.1" />
    <file src="bin\$Configuration$\netcoreapp3.0\*.*" target="tools/netcoreapp3.0" />
    <file src="bin\$Configuration$\netcoreapp3.1\*.*" target="tools/netcoreapp3.1" />
    <file src="bin\$Configuration$\net5.0\*.*" target="tools/net5.0" />
  </files>
</package>
